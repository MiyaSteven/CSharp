@model FullWebApp.Models.MessageForm

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        <a href="/">Home</a>
        <a href="/game/people/create">Create New Character</a>
        <a href="/game/people/message/create">Create New Message</a>
    </div>
    <h1>Create Your Message!</h1>
    <form asp-action="MessageSubmission" asp-controller="Home" method="post">
        <span asp-validation-for="Name"></span>
        <label asp-for="Name"></label>
        <input asp-for="Name">
        <br>
        <span asp-validation-for="Email"></span>
        <label asp-for="Email"></label>
        <input asp-for="Email">
        <br>
        <span asp-validation-for="Company"></span>
        <label asp-for="Company"></label>
        <input asp-for="Company">
        <br>
        <span asp-validation-for="Message"></span>
        <label asp-for="Message"></label>
        <input asp-for="Message">
        <br>
        <span asp-validation-for="PriorityLevel"></span>
        <label asp-for="PriorityLevel"></label>
        <input asp-for="PriorityLevel">
        <br>
        <input type="submit" value="Submit Message">
    </form>
</body>
</html>
