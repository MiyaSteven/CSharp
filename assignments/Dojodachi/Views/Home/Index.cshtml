@model string
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
    <p>
        Fullness: @ViewBag.MyDachi.Name
    </p>
    <p>
        Happiness: @ViewBag.MyDachi.Happiness
    </p>    
    <p>
        Meals: @ViewBag.MyDachi.Meals
    </p>    
    <p>
        Energy: @ViewBag.MyDachi.Energy
    </p>
    </div>
    <div>
        <p>@ViewBag.DachiFeed</p>
        <p>@ViewBag.DachiPlay</p>
        <p>@ViewBag.DachiWork</p>
        <p>@ViewBag.DachiSleep</p>
    </div>
    <button>
        <a href="/feed">Feed</a>
    </button>    
    <button>
        <a href="/play">Play</a>
    </button>    
    <button>
        <a href="/work">Work</a>
    </button>    
    <button>
        <a href="/sleep">Sleep</a>
    </button>
    <button>
        <a href="/restart">Restart</a>
    </button>
</body>
</html>